@model UnitOfWorkExample.Web.Models.Home.IndexViewModel
@using UnitOfWorkExample.Web.Helpers
@{
    ViewBag.Title = "MVC5 Unit of Work Example";
}
<div class="jumbotron">
    <div class="container">
        <h1>MVC5 Unit of Work Example</h1>
    </div>
</div>
<div class="container">
    <h2>Products</h2>
    @if (Model.Products.AnySafe())
    {
        <ul>
            @foreach (var product in Model.Products)
            {
                <li>@product.Name</li>
            }
        </ul>
    }
</div>
<h3>Welcome to games for cash!</h3>
<div class="btn-div">
    <form class="form" action="~/Account/Index">
        <input type="submit" class="btn btn-success buttons" value="Login">
    </form>
    <form class="form" action="~/Account/Register">
        <input type="submit" class="btn btn-danger buttons" value="Register">
    </form>
</div>


<div class="container">
    <input type="text" id="message"/>
    <input type="button" id="sendmessage" value="Send"/>
    <input type="hidden" id="displayname"/>
    <ul id="discussion"></ul>
</div>

<script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>
<!--Reference the autogenerated SignalR hub script. -->
<script src="~/signalr/hubs"></script>
<script src="~/Scripts/test.js"></script>